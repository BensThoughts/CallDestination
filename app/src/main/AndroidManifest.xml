        </activity>

        <activity android:name=".PlacePickerActivity"
                  android:theme="@android:style/Theme.NoDisplay"/>

        <service android:name="com.sanfranciscosunrise.silente.calldestination.OverlayShowingService"
                 android:exported="false"/>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyC17067beytGa8UQpjt-oO6mCi_m4M5x7E"/>


